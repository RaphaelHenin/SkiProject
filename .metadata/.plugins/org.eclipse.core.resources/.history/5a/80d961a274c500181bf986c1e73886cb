import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.PrintStream;
import java.util.HashMap;

public class writeInCSVFile {
	public void write(Node node, HashMap<Integer, Node> datas) throws FileNotFoundException {
		File csvFile = new File("adjacentMatrix.csv");
		if (!csvFile.exists())
			throw new FileNotFoundException("Le fichier n'existe pas");
		else {
			PrintStream l_out = new PrintStream(new FileOutputStream("adjacentMatrix.csv", true));
			for(int i=0; i<37;i++) {
				if(node.adjacentNodes.keySet())
			}
			l_out.print("cellule1,");
			l_out.print("cellule2");

			l_out.flush();
			l_out.close();
			l_out = null;
		}
	}
}
